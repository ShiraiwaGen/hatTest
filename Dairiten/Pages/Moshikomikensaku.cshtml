            <div class="komoku3 text-white mb-1">カナ</div>
            <div class="atai2 mb-1">
                <div class="row mb-3">
                  <div class="col-sm-9">
                    <input type="text" class="form-control form-control-sm" id="KeiyakushameiKana">
                  </div>
                  <label for="KeiyakushameiKana" class="col-sm-3 col-form-label col-form-label-sm" style="color:red">(半角カナ)</label>
                </div>
            </div>
            <div class="komoku3 text-white mb-1">漢字</div>
            <div class="atai2 mb-1">
                <div class="row mb-3">
                  <div class="col-sm-9">
                    <input type="text" class="form-control form-control-sm" id="Keiyakushamei">
                  </div>
                  <label for="Keiyakushamei" class="col-sm-3 col-form-label col-form-label-sm" style="color:red">(全角)</label>
